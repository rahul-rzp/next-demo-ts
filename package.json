{
  "name": "next-demo-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --debug",
    "start": "next start",
    "lint": "next lint",
    "docker": "yarn run docker:build && yarn run docker:run",
    "docker:build": "docker build -t nextjs-docker .",
    "docker:run": "yarn run docker:remove-old-container && docker run -itd -p 3000:3000 --name nextjs-docker-container nextjs-docker",
    "docker:remove-old-container": "docker rm -f nextjs-docker-container",
    "docker:logs": "docker logs nextjs-docker-container"
  },
  "dependencies": {
    "@fontsource/lato": "4.5.8",
    "@razorpay/blade": "0.13.0",
    "axios": "^0.27.2",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "next-transpile-modules": "^9.0.0",
    "typescript": "4.7.4"
  }
}
